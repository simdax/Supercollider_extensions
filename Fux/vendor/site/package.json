{
  "name": "vue-parcel-starter",
  "version": "1.0.0",
  "description": "Vue.js starter kit with Parcel + Typescript",
  "scripts": {
    "build": "parcel build ./front/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "start": "parcel ./front/index.html",
    "dev": "cd server && nodemon outdir --ignore .cache --ignore dist"
  },
  "keywords": [
    "vue",
    "parcel",
    "typescript"
  ],
  "author": "Devon Bellizio",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.1.4",
    "@types/websocket": "^0.0.39",
    "@types/ws": "^5.1.1",
    "command-exists": "^1.2.6",
    "express": "^4.16.3",
    "express-ws": "^3.0.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "json2xml": "^0.1.3",
    "midi-file-parser": "^1.0.0",
    "midiconvert": "^0.4.5",
    "midijs": "^0.12.0",
    "osc-js": "^1.2.2",
    "pouchdb": "^6.4.3",
    "pouchdb-find": "^6.4.3",
    "pouchdb-upsert": "https://github.com/pouchdb/upsert",
    "pug": "^2.0.3",
    "tone": "^0.12.80",
    "vexflow": "^1.2.84",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-hot-reload-api": "^2.3.0",
    "vue-property-decorator": "^6.0.0",
    "vuex": "^3.0.1",
    "x2js": "^3.2.1",
    "xml-js": "^1.6.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.0",
    "typescript": "^2.8.1",
    "vue-template-compiler": "^2.5.16"
  }
}
