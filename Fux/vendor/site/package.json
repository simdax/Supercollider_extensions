    "json2xml": "^0.1.3",
    "midijs": "^0.12.0",
    "pouchdb": "^6.4.3",
    "pug": "^2.0.3",
    "vexflow": "^1.2.84",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-hot-reload-api": "^2.3.0",
    "vue-property-decorator": "^6.0.0",
    "vuex": "^3.0.1",
    "x2js": "^3.2.1",
    "xml-js": "^1.6.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
